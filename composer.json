{
    "name": "ahsanabrar/laravel-neoquent",
    "description": "Laravel Eloquent Wrapper for Neo4j Graph Database.",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "laravel-neoquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahsan Abrar",
            "email": "iahsanabrar@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "illuminate/database": "6.20.26",
        "illuminate/pagination": "5.6.*",
        "illuminate/events": "5.6.*",
        "illuminate/console": "5.6.*",
        "heydavid713/neo4jphp": "0.1.*",
        "graphaware/neo4j-php-client": "4.8.*",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Ahsan\\Neoquent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahsan\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ahsan\\Neoquent\\NeoquentServiceProvider"
            ]
        }
    }
}
